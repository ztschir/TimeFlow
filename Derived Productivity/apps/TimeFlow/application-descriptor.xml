<!-- Attribute "id" must be identical to application folder name -->
<application xmlns="http://www.worklight.com/application-descriptor" id="TimeFlow" platformVersion="5.0">
    <displayName>TimeFlow</displayName>
    <description>TimeFlow</description>
    <author>
        <name>application's author</name>
        <email>application author's e-mail</email>
        <homepage>http://mycompany.com</homepage>
        <copyright>Copyright My Company</copyright>
    </author>
    <height>460</height>
    <width>320</width>
    <mainFile>TimeFlow.html</mainFile>
    <thumbnailImage>common/images/thumbnail.png</thumbnailImage>
    <usage requireAuthentication="never"/>
    <iphone bundleId="com.TimeFlow" version="1.0">
        <worklightSettings include="true"/>
        <security>
            <testAppAuthenticity enabled="false"/>
            <encryptWebResources enabled="false"/>
            <testWebResourcesChecksum enabled="false" ignoreFileExtensions="png, jpg, jpeg, gif, mp4, mp3"/>
        </security>
    </iphone>
    <worklightServerRootURL>http://${local.IPAddress}:8080</worklightServerRootURL>
</application>
